{
  "name": "profiles",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "check-types": "tsc --noEmit",
    "dev": "nest start --watch",
    "lint": "eslint . --max-warnings 0",
    "start": "node dist/main",
    "test": "jest",
    "test:ci": "jest --ci --runInBand --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest --config ./test/jest.config.ts",
    "test:integration:ci": "jest --config ./test/jest.config.ts --ci --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.9",
    "@nestjs/core": "^10.4.9",
    "@nestjs/platform-express": "^10.4.9",
    "@nestjs/swagger": "^8.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.6.1",
    "@itddd/eslint-config": "workspace:*",
    "@itddd/typescript-config": "workspace:*",
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.9",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.14.0",
  "engines": {
    "node": ">=22"
  }
}
